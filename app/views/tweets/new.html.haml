= render "layouts/header" 
.contents-new
  = form_with(model: @tweet, local: true) do |form|
    %h3 投稿する
    .field
      = form.text_field :image, placeholder: "Image Url",class: "form"
    .field
      = form.text_area :text, placeholder: "text" , rows: "10",class: "form text"
    .field
      = form.submit "SEND",class: "btn"
